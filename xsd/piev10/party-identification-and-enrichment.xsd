   </xs:annotation>
   <xs:element name="PieMessage">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A File in the DDEX Party Standard, containing information about one or more Parties.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element name="MessageHeader" type="pie:MessageHeader">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">The MessageHeader for the PieMessage.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="MetadataSourceList"
                        minOccurs="0"
                        type="pie:MetadataSourceList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of one or more Parties that created the metadata.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="PartyList" type="pie:PartyList">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing detailed information about one or more Parties.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:sequence>
         <xs:attribute name="AvsVersionId" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Identifier of the Version of the AllowedValueSets used for the Message. This is represented in an XML schema as an XML Attribute.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="LanguageAndScriptCode" use="required">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Language and script for the Elements of this File as defined in IETF RfC 5646. Language and Script are provided as lang[-script][-region][-variant]. This is represented in an XML schema as an XML Attribute.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:pattern value="[a-zA-Z]{2,3}(-[a-zA-Z]+){0,1}(-[a-zA-Z]{2}|-[0-9]{3}){0,1}(-[a-zA-Z][a-zA-Z0-9]{4}[a-zA-Z0-9]*){0,1}"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="PieRequestMessage">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A File in the DDEX Party Standard, containing a request for information about one or more Parties.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element name="MessageHeader" type="pie:MessageHeader">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">The MessageHeader for the PieRequestMessage.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="RequestedParty"
                        maxOccurs="unbounded"
                        type="pie:RequestedParty">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a Party for which information is requested.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:sequence>
         <xs:attribute name="AvsVersionId" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Identifier of the Version of the AllowedValueSets used for the Message. This is represented in an XML schema as an XML Attribute.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="LanguageAndScriptCode" use="required">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Language and script for the Elements of this File as defined in IETF RfC 5646. Language and Script are provided as lang[-script][-region][-variant]. This is represented in an XML schema as an XML Attribute.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:pattern value="[a-zA-Z]{2,3}(-[a-zA-Z]+){0,1}(-[a-zA-Z]{2}|-[0-9]{3}){0,1}(-[a-zA-Z][a-zA-Z0-9]{4}[a-zA-Z0-9]*){0,1}"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="Feed">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Message in the Party Identification and Enrichment Standard for an atom feed defined by W3C/IETF.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:choice minOccurs="3" maxOccurs="unbounded">
            <xs:element name="author"
                        minOccurs="0"
                        maxOccurs="unbounded"
                        type="pie:Person">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of the Name, Identifier and Role(s) of an author of a Resource.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="category"
                        minOccurs="0"
                        maxOccurs="unbounded"
                        type="pie:Category">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a category.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="contributor"
                        minOccurs="0"
                        maxOccurs="unbounded"
                        type="pie:Person">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of the Name, Identifier and Role(s) of a contributor to a Resource.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="generator" minOccurs="0" type="pie:Generator">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a generator.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="icon" minOccurs="0" type="pie:Icon">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of an icon.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="id" type="pie:Id">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of an ID.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="link" minOccurs="0" maxOccurs="unbounded" type="pie:Link">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a link.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="logo" minOccurs="0" type="pie:Logo">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a logo.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="rights" minOccurs="0" type="pie:Text">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of rights.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="subtitle" minOccurs="0" type="pie:Text">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a subtitle.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="title" type="pie:Text">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a title.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="updated" type="pie:DateTime">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of an update.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="entry"
                        minOccurs="0"
                        maxOccurs="unbounded"
                        type="pie:Entry">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of an entry.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:any namespace="##other" minOccurs="0" maxOccurs="unbounded"/>
         </xs:choice>
         <xs:anyAttribute namespace="##other"/>
      </xs:complexType>
   </xs:element>
   <xs:complexType name="Contribution">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a contribution.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Role" maxOccurs="unbounded" type="pie:ContributorRole">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a Role played by the Contributor in the creation of the Resource.</xs:documentation>
               <xs:documentation source="ddex:Comment">Further Reading: https://kb.ddex.net/implementing-each-standard/best-practices-for-all-ddex-standards/guidance-on-contributors%2C-artists-and-writers/information-on-displayartists%2C-displayartistnames%2C-contributors-and-indirectcontributors</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="IsPrimaryRole" minOccurs="0" type="xs:boolean">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Flag indicating whether the contribution (role) is the primary contribution (role) that a Party makes in the creation of the Resource (=true) or not (=false).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="HasMadeFeaturedContribution" minOccurs="0" type="xs:boolean">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Flag indicating whether the contribution in the creation of the Resource is made by a Party that is a featured Artist (=true) or not (=false).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="HasMadeContractedContribution"
                     minOccurs="0"
                     type="xs:boolean">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Flag indicating whether the contribution in the creation of the Resource is made by a Party that is an Artist that has a contract for its part in creating a Resource (=true) or not (=false).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Event" minOccurs="0" type="pie:EventDate">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of the Date of the Event in which the contribution to the creation of the Resource was made. This is a string with the syntax YYYY[-MM[-DD]].</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="CreationDescription">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a Description for a Creation.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="Title"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="pie:TitleWithUDV">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a Title.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DisplayArtistName" minOccurs="0" type="pie:DisplayArtistName">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing the Name used when presenting Artist details of the Creation to a Consumer.</xs:documentation>
               <xs:documentation source="ddex:Comment">Further Reading: https://kb.ddex.net/implementing-each-standard/best-practices-for-all-ddex-standards/guidance-on-contributors%2C-artists-and-writers/communicating-displayartists-and-displayartistname</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PublicationDate" minOccurs="0" type="pie:EventDate">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of the Date and Place at which the Creation was published. This is a string with the syntax YYYY[-MM[-DD]].</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DetailedPartyIdForParty">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a PartyId.</xs:documentation>
         <xs:documentation source="ddex:Comment">Explanatory Note: This Composite is named DetailedPartyIdForParty to disambiguate it from the basic PartyId Composite.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ISNI" minOccurs="0" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An International Standard Name Identifier, the ISO 27729 Standard Identifier for names. DDEX will enforce the syntax [0-9]{15}[X0-9] using XML Schema in the future.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DPID" minOccurs="0">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Identifier of a Party according to the DdexPartyId standard DDEX-DPID.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:pattern value="PADPIDA[a-zA-Z0-9]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="IpiNameNumber" minOccurs="0">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Interested Party Identifier, a CISAC standard Identifier. An IpiNameNumber comprises 11 digits.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:pattern value="[0-9]{11}"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="IPN" minOccurs="0" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An International Performer Number, an IPDA Identifier.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CisacSocietyId" minOccurs="0" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A CISAC Society Identifier, a CISAC standard Identifier for music rights societies.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ProprietaryId"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="pie:ProprietaryId">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a ProprietaryIdentifier of the Party.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="IsDeprecated" type="xs:boolean">
         <xs:annotation>
            <xs:documentation source="ddex:Definition">The Flag indicating whether this Composite provides a deprecated values (=true). This Flag should not be set if this is not the case. This is represented in an XML schema as an XML Attribute.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="Entry">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of an entry.</xs:documentation>
      </xs:annotation>
      <xs:choice maxOccurs="unbounded">
         <xs:element name="author"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="pie:Person">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of the Name, Identifier and Role(s) of an author of a Resource.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="category"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="pie:Category">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a category.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="content" minOccurs="0" type="pie:Content">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a content.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="contributor"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="pie:Person">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of the Name, Identifier and Role(s) of a contributor to a Resource.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="id" type="pie:Id">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of an ID.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="link" minOccurs="0" maxOccurs="unbounded" type="pie:Link">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a link.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="published" minOccurs="0" type="pie:DateTime">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a published.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="rights" minOccurs="0" type="pie:Text">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of rights.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="source" minOccurs="0" type="pie:Source">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a source.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="summary" minOccurs="0" type="pie:Text">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a summary.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="title" type="pie:Text">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a title. The title should contain three pieces of information separated by a semicolon and a space character: (1) the DPID of the message sender (without dashes); (2) the subscription ID; and (3) the Date when the entry was added to the feed queue (in the ISO format yyyy-mm-dd).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="updated" type="pie:DateTime">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of an update.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SubscriptionId" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Identifier of the subscription to which the Notification is a reply.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Party" type="pie:PartyDescriptorForEntry">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a Party. This is either the only entity about which the relevant PieMessage talks or the main one.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:choice>
      <xs:attribute name="AvsVersionId" type="xs:string" use="required">
         <xs:annotation>
            <xs:documentation source="ddex:Definition">The Identifier of the Version of the AllowedValueSets used for the Message. This is represented in an XML schema as an XML Attribute.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:anyAttribute namespace="##other"/>
   </xs:complexType>
   <xs:complexType name="Event">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of an Event.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MetadataSourceReference"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="pie:MetadataSourceReference">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a Party (specific to this Message) that is the author of this metadata. This is a LocalMetadataSourceAnchorReference starting with the letter U.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:choice>
            <xs:element name="Date" type="pie:EventDate">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of the Date and Place of the Event. The StartDate must be no later than the EndDate if both are provided.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:sequence>
               <xs:element name="StartDate" minOccurs="0" type="pie:EventDate">
                  <xs:annotation>
                     <xs:documentation source="ddex:Definition">A Composite containing details of the Date and Place of the beginning of the Period in which the Event occurs (in ISO 8601 format: YYYY-MM-DD). The StartDate must be no later than the EndDate if both are provided.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="EndDate" minOccurs="0" type="pie:EventDate">
                  <xs:annotation>
                     <xs:documentation source="ddex:Definition">A Composite containing details of the Date and Place of the end of the Period in which the Event occurs (in ISO 8601 format: YYYY-MM-DD). The EndDate must not be earlier than the StartDate if both are provided.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:choice>
         <xs:element name="EventType" type="pie:EventType">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Type of the Event.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="EventDescription"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="pie:Description">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a Description of the Event.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="EventType">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of an EventType.</xs:documentation>
      </xs:annotation>
      <xs:simpleContent>
         <xs:extension base="avs:EventType">
            <xs:attribute name="Namespace" type="xs:string">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">The Namespace of the EventType. This is represented in an XML schema as an XML Attribute.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="UserDefinedValue" type="xs:string">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A UserDefined value of the EventType. This is represented in an XML schema as an XML Attribute.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
   <xs:complexType name="Focus">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a focus track.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MetadataSourceReference"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="pie:MetadataSourceReference">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for a Party (specific to this Message) that is the author of this metadata. This is a LocalMetadataSourceAnchorReference starting with the letter U.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="FocusTrack" minOccurs="0" type="pie:ResourceSummary">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a Resource.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="FocusRelease" minOccurs="0" type="pie:ReleaseSummary">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a Release.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="FocusWork" minOccurs="0" type="pie:WorkSummary">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a MusicalWork.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DisplayArtistName"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="pie:DisplayArtistNameWithPronunciation">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing the Name to be used by a DSP when presenting Artist details of the Release to a Consumer.</xs:documentation>
               <xs:documentation source="ddex:Comment">Further Reading: https://kb.ddex.net/implementing-each-standard/best-practices-for-all-ddex-standards/guidance-on-contributors%2C-artists-and-writers/communicating-displayartists-and-displayartistname</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DisplayArtist"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="pie:PartyDescriptorWithPronunciation">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a DisplayArtist.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Writer"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="pie:PartyDescriptorWithPronunciation">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a writer of the Work for which information is provided.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SequenceNumber" minOccurs="0" type="xs:integer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The number indicating the order of the focus Track in a group of Tracks. It should not be provided unless more than one focus track is provided for a specific Artist.</xs:documentation>
            </xs:annotation>
         </xs:element>
